	<select id="selectAppKgssSyzbEntityByQo" parameterType="huacloud.jwdsj.module.sy.ks.model.qo.SyKsQo"
	resultMap="AppKgssSyzbTotalMap">
	select <include refid="Base_Column_List_sum" /> ,t.RQTJ
	from app.APP_KGSS_SYZB t
	where 1=1 
	<if test="syKsQo.dwbm != null ">
		and t.CBDW_BM like CONCAT(CONCAT('',#{syKsQo.dwbm}),'%')
	</if>
	<if test="syKsQo.beginDate!=null" >
		and t.RQTJ BETWEEN  #{syKsQo.beginDate} AND  #{syKsQo.endDate} 
	</if>
	group by t.RQTJ
	
	order by RQTJ desc limit 10
	</select>
	
	
	<select id="selectAppKgssSyzbTotalByQo" parameterType="huacloud.jwdsj.module.sy.ks.model.qo.SyKsQo"
	resultMap="AppKgssSyzbTotalMap">
		select
			<include refid="Base_Column_List_sum" />
		from
	        app.APP_KGSS_SYZB t
	        where 1=1 
		<if test="syKsQo.dwbm != null">
			and t.CBDW_BM like CONCAT(CONCAT('',#{syKsQo.dwbm}),'%')
		</if>
		<if test="syKsQo.beginDate!=null" >
			and t.RQTJ BETWEEN  #{syKsQo.beginDate} AND  #{syKsQo.endDate} 
		</if>
	</select>
